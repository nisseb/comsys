N = 1000;
q = 0.3;

timesteps = 100000;

for q=0:0.01:

y = zeros(1, timesteps);  % number of iphone users
y(1) = 0;  % intitial condition

mean_field = @(x) q/2*(N-x)/N+(N-x)/N*(1-q)*x^2/N^2 ...
                  - q*x/(2*N) - x/N * (1-q)*((N-x)/N)^2;

for t=2:timesteps
   y(t) = y(t-1) + mean_field(y(t-1));
end

plot(y)